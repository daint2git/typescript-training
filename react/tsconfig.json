{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "jsx": "react",
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noImplicitAny": true
  },
  "exclude": ["node_modules"],
  "include": ["src"]
}
